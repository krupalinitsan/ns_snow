{
  "name": "nitsan/ns-snow",
  "type": "typo3-cms-extension",
  "description": "Add nice looking animation effect of falling snow to your TYPO3 site and enjoy winter as well as Christmas.",
  "homepage": "https://extensions.typo3.org/extension/ns_snow/",
  "license": "GPL-2.0+",
  "keywords": [
    "TYPO3 CMS",
    "Snow fall",
    "Nitsan",
    "Snow"    
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_snow/issues"
  },
  "require": {
    "typo3/cms-core": ">=6.2.0"
  },
  "autoload": {
    "psr-4": {
      "Nitsan\\NsSnow\\": "Classes/"
    }
  },
  "replace": {
      "ns_snow": "self.version",
      "nitsan/ns-snow": "self.version"
  },
  "extra": {
      "typo3/cms": {
          "extension-key": "ns_snow"
      }
  }
}